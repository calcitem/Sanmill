{
  "_description": "迁移学习示例配置",
  "_mode": "Transfer learning from pretrained model",
  "_usage": "python train_nnue.py --config configs/transfer_learning_example.json --transfer-from path/to/pretrained.checkpoint",
  
  "pipeline": false,
  "data": "training_data.txt",
  "output": "models/transfer_learned_model.bin",
  
  "epochs": 100,
  "batch-size": 8192,
  "lr": 0.001,
  "lr-scheduler": "adaptive",
  "lr-auto-scale": false,
  
  "_comment_transfer": "迁移学习参数通过命令行指定",
  "transfer-strategy": "fine-tune",
  "transfer-lr-scale": 0.1,
  
  "feature-size": 115,
  "hidden-size": 256,
  "val-split": 0.1,
  
  "device": "auto",
  
  "plot": true,
  "plot-interval": 10,
  "save-csv": true,
  "show-plots": false,
  
  "_transfer_strategies": {
    "_description": "可用的迁移学习策略",
    "full": "完全迁移：加载所有兼容的权重",
    "freeze-input": "冻结输入层：只训练隐藏层和输出层",
    "freeze-hidden": "冻结隐藏层：只训练输入层和输出层",
    "fine-tune": "微调：加载权重但使用很小的学习率"
  },
  
  "_usage_examples": {
    "basic_transfer": "python train_nnue.py --config configs/transfer_learning_example.json --transfer-from models/pretrained.checkpoint",
    "full_transfer": "python train_nnue.py --config configs/transfer_learning_example.json --transfer-from models/pretrained.checkpoint --transfer-strategy full",
    "freeze_input": "python train_nnue.py --config configs/transfer_learning_example.json --transfer-from models/pretrained.checkpoint --transfer-strategy freeze-input --transfer-lr-scale 0.05"
  }
}
